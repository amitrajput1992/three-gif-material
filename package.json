    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "detect-circular-deps": "^1.3.0",
    "eslint": "^5.13.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "flow-bin": "^0.95.1",
